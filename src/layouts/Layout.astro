
---
import { metas } from '../../includes/01-metas';
import { analiticas } from '../../includes/02-analitica';
import { publi } from '../../includes/03-publi';
import '../styles/main.scss';


interface Props {
	title: string;
}

const { title } = Astro.props;
---
<!doctype html>
<html lang="en">
	<head>
		<meta charset="UTF-8" />
		<meta name="viewport" content="width=device-width" />
		<link rel="icon" type="image/svg+xml" href="/favicon.svg" />
		<meta name ="analiticas" content={analiticas} />
		<meta name ="metas" content={metas} />
		<meta name ="publi" content={publi} />
		
		
		<title>{title}</title>
		
	</head>
	<body>
		<slot />
	</body>
</html>

